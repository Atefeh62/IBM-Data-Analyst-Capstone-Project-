        "SELECT ConvertedComp,Age\n",
        "FROM master\n",
        "where age >= 45 and age <= 60\n",
        "order by age\n",
        "\"\"\"\n",
        "DATA=pd.read_sql_query(QUERY,conn)\n",
        "\n",
        "DATA.groupby('Age').median().plot.bar(stacked=False)\n",
        "plt.title('Median compensation in $, by Age')\n",
        "\n",
