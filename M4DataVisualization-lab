            "needs_background": "light"
          }
        }
      ],
      "source": [
        "QUERY = \"\"\"\n",
        "SELECT CodeRevHrs,WorkWeekHrs,Age\n",
        "FROM master\n",
        "where age >= 30 and age <= 35\n",
        "order by age\n",
        "\"\"\"\n",
        "DATA=pd.read_sql_query(QUERY,conn)\n",
        "\n",
        "\n",
        "\n",
        "\n",
        "DATA.groupby('Age').mean().plot.bar(stacked=True)\n",
        "plt.title('Mean hours, by Age')\n",
        " \n",
        "\n"
      ]
    },
